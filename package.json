  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "cross-env NODE_ENV=development vite --config client/vite.config.ts",
    "dev:admin": "cross-env NODE_ENV=development vite --config client-admin/vite.config.ts",
    "build": "npm run prisma:generate && npm run build:client && npm run build:admin && npm run build:server",
    "build:client": "npm --prefix client run build",
    "build:admin": "npm --prefix client-admin run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "postbuild": "npx prisma generate",
    "aws:build": "npm run build && npm run postbuild",
    "aws:start": "node dist/index.js",
    "check": "tsc --skipLibCheck",
    "db:push": "drizzle-kit push",
    "db:migrate": "npx prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -- npx prisma migrate dev",
    "prisma:generate": "npx prisma generate"
  },
